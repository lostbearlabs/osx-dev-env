#!/usr/bin/env ruby

require_relative('./git-helpers.rb')

unless ARGV[0]
  puts 'Usage: git-set-parent-build <build number>'
  exit
end

current_branch = git_current_branch
parent_build= ARGV[0]
set_parent_build(current_branch, parent_build)

puts "parent build set to #{get_parent_build(current_branch)}"

